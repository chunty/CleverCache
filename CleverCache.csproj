<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>CleverCache</PackageId>
		<Version>1.0.4</Version>
		<Authors>Chris Hunt</Authors>
		<Company>TappetyClick</Company>
		<Description>Like MemoryCache but better! You never need to worry about clearing cache or race conditions again.</Description>
		<PackageTags>MemoryCache,Automatic Cache</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.3.0" />
		<PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="9.0.2" />
		<PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.2" />
	</ItemGroup>
</Project>
